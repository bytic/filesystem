{
  "name": "bytic/filesystem",
  "type": "library",
  "description": "filesystem module for bytic framework",
  "homepage": "https://github.com/bytic/filesystem",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\Filesystem\\": "src"
    },
    "classmap": [
      "legacy"
    ],
    "files": [
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\Filesystem\\Tests\\": "tests/src"
    }
  },
  "require": {
    "php": "^7.0",
    "bytic/config": "^1.0",
    "bytic/container": "^1.0",
    "bytic/utility": "^1.0",
    "symfony/filesystem": "^4.2",
    "league/flysystem": "^1.0"
  },
  "require-dev": {
    "league/flysystem-aws-s3-v3": "^1.0",
    "squizlabs/php_codesniffer": "^3.0",
    "phpunit/phpunit": "^7.3",
    "mockery/mockery": "^1.1"
  },
  "suggest": {
    "league/flysystem-aws-s3-v3": "Required to use the Flysystem S3 driver (^1.0).",
    "league/flysystem-cached-adapter": "Required to use the Flysystem cache (^1.0).",
    "league/flysystem-sftp": "Required to use the Flysystem SFTP driver (^1.0)."
  }
}
